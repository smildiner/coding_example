	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/bimj.201700223},
	keywords = {LMM, frequentist, clustered data, health status assessment, nonignorable dropout, random-effects approach, MLMM},
	pages = {962--978},
	file = {Snapshot:/Users/a6159737/Zotero/storage/V6JYUHZZ/bimj.html:text/html;Submitted Version:/Users/a6159737/Zotero/storage/H2UP7FP7/Montanari et al. - 2018 - A multilevel latent Markov model for the evaluatio.pdf:application/pdf},
