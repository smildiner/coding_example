	pages = {3120--3130},
	file = {IEEE Xplore Abstract Record:/Users/a6159737/Zotero/storage/3DXW29EQ/9336287.html:text/html;IEEE Xplore Full Text PDF:/Users/a6159737/Zotero/storage/M5I4QRBV/Yu - 2022 - Explicit Duration Recurrent Networks.pdf:application/pdf},
