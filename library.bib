	keywords = {Bison bison, maximum likelihood, random walk, telemetry data, random effects, state-space model, behavioral state, semi-Markov model},
	pages = {2336--2342},
	file = {Full Text PDF:/Users/a6159737/Zotero/storage/44MNPIMC/Langrock et al. - 2012 - Flexible and practical modeling of animal telemetr.pdf:application/pdf;Snapshot:/Users/a6159737/Zotero/storage/DV48RDPX/11-2241.html:text/html},
