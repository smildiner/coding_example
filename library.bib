	pages = {515--534},
	file = {Full Text PDF:/Users/a6159737/Zotero/storage/ETJF4ICZ/Gelman - 2006 - Prior distributions for variance parameters in hie.pdf:application/pdf;Snapshot:/Users/a6159737/Zotero/storage/BYDXFKZ2/06-BA117A.html:text/html},
