	note = {Publisher: MDPI AG},
	keywords = {Semi-supervised learning, Activity recognition, Benchmark database, Cyclic activities, Gait analysis, Gait phases, Home monitoring, Inertial measurement unit, Smart annotation},
	file = {PDF:/Users/a6159737/Zotero/storage/R39GYIBB/Martindale, Sprager, Eskofier - 2019 - Hidden markov model-based smart annotation for benchmark cyclic activity recognition database usi.pdf:application/pdf},
}

@article{Baratchi2014,
