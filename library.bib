	author = {Stanislaus, Sharleny and Faurholt-Jepsen, Maria and Vinberg, Maj and Coello, Klara and Kjærstad, Hanne Lie and Melbye, Sigurd and Sletved, Kimie S. O. and Christensen, Ellen Margrethe and Frost, Mads and Bardram, Jakob E. and Kessing, Lars Vedel},
	month = jun,
	year = {2020},
	keywords = {Smartphone, Bipolar disorder, Mood instability, Electronic monitoring, Self-reports},
	pages = {336--344},
	file = {ScienceDirect Snapshot:/Users/a6159737/Zotero/storage/BAZMDXI7/S0165032719320348.html:text/html;Stanislaus et al. - 2020 - Mood instability in patients with newly diagnosed .pdf:/Users/a6159737/Zotero/storage/BDHRTXBY/Stanislaus et al. - 2020 - Mood instability in patients with newly diagnosed .pdf:application/pdf},
