	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/00273171.2019.1660606},
	keywords = {Latent Markov models, sensitivity analysis, informative missingness, random effects, repeated observations},
	pages = {647--663},
	file = {Snapshot:/Users/a6159737/Zotero/storage/8J9UHWUM/00273171.2019.html:text/html},
}

