	pages = {2441--2452},
	file = {Kemere et al. - 2008 - Detecting Neural-State Transitions Using Hidden Ma.pdf:/Users/a6159737/Zotero/storage/CG9QKX3J/Kemere et al. - 2008 - Detecting Neural-State Transitions Using Hidden Ma.pdf:application/pdf},
}

@article{quinn_task-evoked_2018,
	title = {Task-{Evoked} {Dynamic} {Network} {Analysis} {Through} {Hidden} {Markov} {Modeling}},
