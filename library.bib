	keywords = {Activity recognition, Feature extraction, Acceleration, bipolar disorder, Accuracy, Biomedical measurement, depression recognition, Informatics, mental disease monitoring, Monitoring, Mood, mood recognition, smartphones, wearable computing},
	pages = {140--148},
