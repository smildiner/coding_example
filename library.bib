	journal = {Learning and Individual Differences},
	author = {Bray, Bethany C. and Dziak, John J.},
	month = aug,
	year = {2018},
	keywords = {Learning, Latent class analysis, Latent transition analysis, Individual differences, Latent profile analysis},
	pages = {105--110},
	file = {Bray and Dziak - 2018 - Commentary on latent class, latent profile, and la.pdf:/Users/a6159737/Zotero/storage/ETV2GHBU/Bray and Dziak - 2018 - Commentary on latent class, latent profile, and la.pdf:application/pdf;ScienceDirect Snapshot:/Users/a6159737/Zotero/storage/6WZRB9CH/S1041608018301006.html:text/html},
}

@article{hickendorff_informative_2018,
