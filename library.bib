	keywords = {Hidden Markov model, Epilepsy, Diffusion model, Mixed effects model, Pharmacokinetics, Stochastic differential equation},
	pages = {91--105},
	file = {PDF:/Users/a6159737/Zotero/storage/UFL3CE8Q/Lavielle - 2018 - Pharmacometrics models with hidden Markovian dynamics.pdf:application/pdf},
}

