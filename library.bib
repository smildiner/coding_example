	month = sep,
	year = {2013},
	keywords = {Hidden Markov Model, Detecting Abnormal Behavior, Hierarchy Hidden Markov Model, Ubiquitous Environment, Ubiquitous Home Network, Viterbi Algorithm},
	pages = {1814--1819},
}

@article{maruotti_semiparametric_2009,
	title = {A semiparametric approach to hidden {Markov} models under longitudinal observations},
	volume = {19},
	issn = {09603174},
	url = {https://link-springer-com.proxy.library.uu.nl/article/10.1007/s11222-008-9099-2},
	doi = {10.1007/s11222-008-9099-2},
	abstract = {We propose a hidden Markov model for longitudinal count data where sources of unobserved heterogeneity arise, making data overdispersed. The observed process, conditionally on the hidden states, is assumed to follow an inhomogeneous Poisson kernel, where the unobserved heterogeneity is modeled in a generalized linear model (GLM) framework by adding individual-specific random effects in the link function. Due to the complexity of the likelihood within the GLM framework, model parameters may be estimated by numerical maximization of the log-likelihood function or by simulation methods; we propose a more flexible approach based on the Expectation Maximization (EM) algorithm. Parameter estimation is carried out using a non-parametric maximum likelihood (NPML) approach in a finite mixture context. Simulation results and two empirical examples are provided. © Springer Science+Business Media, LLC 2008.},
	number = {4},
	urldate = {2021-02-17},
