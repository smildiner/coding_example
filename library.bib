	note = {Publisher: John Wiley and Sons Ltd},
	keywords = {Hidden Markov Model, EM algorithm, hidden Markov model, longitudinal data, heart rate, linear mixed model, Em Algorithm, Heart Rate, Linear Mixed Model, Longitudinal Data},
