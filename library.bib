	author = {Schultzberg, Mårten and Muthén, Bengt},
	month = jul,
	year = {2018},
	note = {Publisher: Routledge},
	keywords = {power, between-level mediation model, DSEM, sample size},
	pages = {495--515},
	file = {PDF:/Users/a6159737/Zotero/storage/R8W4LF2A/Schultzberg, Muthén - 2018 - Number of Subjects and Time Points Needed for Multilevel Time-Series Analysis A Simulation Study of Dynami.pdf:application/pdf},
