	pages = {315--341},
	file = {Springer Full Text PDF:/Users/a6159737/Zotero/storage/3IU857SH/Cochran et al. - 2017 - A Comparison of Mathematical Models of Mood in Bip.pdf:application/pdf},
}

@article{lopez-castroman_onset_2019,
	title = {Onset of schizophrenia diagnoses in a large clinical cohort},
	volume = {9},
	copyright = {2019 The Author(s)},
