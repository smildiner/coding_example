	issn = {2452-3100},
	url = {https://www.sciencedirect.com/science/article/pii/S2452310020300135},
	doi = {10.1016/j.coisb.2020.07.008},
	abstract = {Mood disorders present on-going challenges to the medical field, with difficulties ranging from establishing effective treatments to understanding complexities of one's mood. One solution is the use of mobile apps and wearables for measuring physiological symptoms and real-time mood to shape mood and behavior. Current digital research is focused on increasing engagement in monitoring mood, uncovering mood dynamics, predicting mood, and providing digital microinterventions. This review discusses the importance and risks of user engagement, as well as barriers to improving it. Research on mood dynamics highlights the possibility to reveal data-driven computational phenotypes that could guide treatment. Mobile apps are being used to track voice patterns, global positioning system (GPS), and phone usage for predicting mood and treatment response. Future directions include using mobile apps to deliver and evaluate microinterventions. To continue these advances, standardized reporting and study designs should be considered to improve digital solutions for mood disorders.},
	language = {en},
	urldate = {2022-04-21},
	journal = {Current Opinion in Systems Biology},
	author = {Victory, Amanda and Letkiewicz, Allison and Cochran, Amy L.},
	month = jun,
	year = {2020},
	keywords = {Machine learning, Smartphone, Bipolar disorder, Behavioral research, Computational phenotype, Fitness trackers, Major depressive disorder, Mobile applications, Mood disorders},
	pages = {25--31},
}

@article{cochran_testing_2018,
	title = {Testing frameworks for personalizing bipolar disorder},
	volume = {8},
	copyright = {2018 The Author(s)},
	issn = {2158-3188},
	url = {https://www.nature.com/articles/s41398-017-0084-4},
	doi = {10.1038/s41398-017-0084-4},
	abstract = {The hallmark of bipolar disorder is a clinical course of recurrent manic and depressive symptoms of varying severity and duration. Mathematical modeling of bipolar disorder holds the promise of an ability to personalize diagnoses, to predict future mood episodes, to directly compare diverse datasets, and to link basic mechanisms to behavioral data. Several modeling frameworks have been proposed for bipolar disorder, which represent competing hypothesis about the basic framework of the disorder. Here, we test these hypotheses with self-report assessments of mania and depression symptoms from 178 bipolar patients followed prospectively for 4 or more years. Statistical analysis of the data did not support the hypotheses that mood arises from a rhythmic process or multiple stable states (e.g., mania or depression) or that manic and depressive symptoms are highly anti-correlated. Alternatively, it is shown that bipolar disorder could arise from an inability for mood to quickly return to normal when perturbed. This latter concept is embodied by an affective instability model that can be personalized to the clinical course of any individual with chronic disorders that have an affective component.},
	language = {en},
	number = {1},
	urldate = {2022-05-04},
	journal = {Translational Psychiatry},
	author = {Cochran, Amy L. and Schultz, André and McInnis, Melvin G. and Forger, Daniel B.},
	month = feb,
	year = {2018},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Bipolar disorder, Diagnostic markers},
	pages = {1--10},
	file = {Full Text PDF:/Users/a6159737/Zotero/storage/RRK2M496/Cochran et al. - 2018 - Testing frameworks for personalizing bipolar disor.pdf:application/pdf;Snapshot:/Users/a6159737/Zotero/storage/8CBBW65C/s41398-017-0084-4.html:text/html},
}

@article{harrison_emerging_2018,
	title = {The {Emerging} {Neurobiology} of {Bipolar} {Disorder}},
