@article{kim_hidden_2020,
	title = {A hidden {Markov} model for analyzing eye-tracking of moving objects: {Case} study in a sustained attention paradigm},
	volume = {52},
	issn = {15543528},
	url = {https://doi.org/10.3758/s13428-019-01313-2},
	doi = {10.3758/s13428-019-01313-2},
	abstract = {Eye-tracking provides an opportunity to generate and analyze high-density data relevant to understanding cognition. However, while events in the real world are often dynamic, eye-tracking paradigms are typically limited to assessing gaze toward static objects. In this study, we propose a generative framework, based on a hidden Markov model (HMM), for using eye-tracking data to analyze behavior in the context of multiple moving objects of interest. We apply this framework to analyze data from a recent visual object tracking task paradigm, TrackIt, for studying selective sustained attention in children. Within this paradigm, we present two validation experiments to show that the HMM provides a viable approach to studying eye-tracking data with moving stimuli, and to illustrate the benefits of the HMM approach over some more naive possible approaches. The first experiment utilizes a novel ‘supervised’ variant of TrackIt, while the second compares directly with judgments made by human coders using data from the original TrackIt task. Our results suggest that the HMM-based method provides a robust analysis of eye-tracking data with moving stimuli, both for adults and for children as young as 3.5–6 years old.},
	number = {3},
	urldate = {2020-10-02},
	journal = {Behavior Research Methods},
	author = {Kim, Jaeah and Singh, Shashank and Thiessen, Erik D. and Fisher, Anna V.},
	month = jun,
	year = {2020},
	pmid = {31898297},
	note = {Publisher: Springer},
	keywords = {Hidden Markov model, Eye-tracking, Selective sustained attention, TrackIt, Visual object tracking},
	pages = {1225--1243},
	file = {PDF:/Users/a6159737/Zotero/storage/NPSVRVND/Kim et al. - 2020 - A hidden Markov model for analyzing eye-tracking of moving objects Case study in a sustained attention paradigm.pdf:application/pdf},
}

@article{mcclintock_worth_2020,
	title = {Worth the effort? {A} practical examination of random effects in hidden {Markov} models for animal telemetry data},
	url = {https://doi.org/10.1101/2020.07.10.196410},
	doi = {10.1101/2020.07.10.196410},
	abstract = {1 1. Hidden Markov models (HMMs) that include individual-level random effects have re-2 cently been promoted for inferring animal movement behaviour from biotelemetry data. 3 These "mixed HMMs" come at significant cost in terms of implementation and com-4 putation, and discrete random effects have been advocated as a practical alternative to 5 more computationally-intensive continuous random effects. However, the performance 6 of mixed HMMs has not yet been sufficiently explored to justify their widespread adop-7 tion, and there is currently little guidance for practitioners weighing the costs and 8 benefits of mixed HMMs for a particular research objective. 9 2. We performed an extensive simulation study comparing the performance of a suite of 10 fixed and random effect models for individual heterogeneity in the hidden state process 11 of a 2-state HMM. We focused on sampling scenarios more typical of telemetry studies, 12 which often consist of relatively long time series (30 − 250 observations per animal) for 13 relatively few individuals (5 − 100 animals). 14 3. We generally found mixed HMMs did not improve state assignment relative to stan-15 dard HMMs. Reliable estimation of random effects required larger sample sizes than 16 are often feasible in telemetry studies. Continuous random effect models were fairly 17 robust to data generated under discrete random effects, but not vice versa. Inclusion of 18 random effects to "mop up" unexplained variation can improve estimation of state tran-19 sition probabilities and measurable covariate effects, but discrete random effects can be 20 a relatively poor (and potentially misleading) approximation for continuous variation. 21 4. When weighing the costs and benefits of mixed HMMs, three important consid-22 erations are study objectives, sample size, and model complexity. HMM applications 23 often focus on state assignment with little emphasis on heterogeneity in state transition 24},
	urldate = {2020-10-02},
	author = {Mcclintock, Brett T},
	year = {2020},
	file = {PDF:/Users/a6159737/Zotero/storage/6MKJIZEZ/Mcclintock - 2020 - Worth the effort A practical examination of random effects in hidden Markov models for animal telemetry data.pdf:application/pdf},
}

@article{colombi_multiple_2015,
	title = {Multiple hidden {Markov} models for categorical time series},
	volume = {140},
	issn = {10957243},
	doi = {10.1016/j.jmva.2015.04.002},
	abstract = {We introduce multiple hidden Markov models (MHMMs) where a multivariate categorical time series depends on a latent multivariate Markov chain. MHMMs provide an elegant framework for specifying various independence relationships between multiple discrete time processes. These independencies are interpreted as Markov properties of a mixed graph and a chain graph associated respectively to the latent and observation components of the MHMM. These Markov properties are also translated into zero restrictions on the parameters of marginal models for the transition probabilities and the distributions of observable variables given the latent states.},
	urldate = {2020-10-02},
	journal = {Journal of Multivariate Analysis},
	author = {Colombi, R. and Giordano, S.},
	month = sep,
	year = {2015},
	note = {arXiv: 1309.3895
Publisher: Academic Press Inc.},
	keywords = {Conditional independence, Granger noncausality, Graphical models, Marginal models, Markov properties, Multivariate Markov chains},
	pages = {19--30},
	file = {PDF:/Users/a6159737/Zotero/storage/A23LCDV5/Colombi, Giordano - 2015 - Multiple hidden Markov models for categorical time series(2).pdf:application/pdf;PDF:/Users/a6159737/Zotero/storage/M7TIXJ56/Colombi, Giordano - 2015 - Multiple hidden Markov models for categorical time series.pdf:application/pdf},
