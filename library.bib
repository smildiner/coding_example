	keywords = {Poisson distribution, Time series, Autocorrelation, Count Negative binomial distribution, State-space models},
	pages = {1533--1547},
	file = {Full Text PDF:/Users/a6159737/Zotero/storage/BCYBKSCN/Davis et al. - 2021 - Count Time Series A Methodological Review.pdf:application/pdf},
}

