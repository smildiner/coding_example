	file = {PDF:/Users/a6159737/Zotero/storage/RWXBW89E/Sharma et al. - 2019 - A dataset of continuous affect annotations and physiological signals for emotion analysis.pdf:application/pdf},
}

@article{Hamaker2017a,
	title = {No {Time} {Like} the {Present}: {Discovering} the {Hidden} {Dynamics} in {Intensive} {Longitudinal} {Data}},
