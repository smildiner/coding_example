	journal = {Applied Soft Computing Journal},
	author = {Ignatov, Andrey},
	month = jan,
	year = {2018},
	note = {Publisher: Elsevier Ltd
ISBN: 9781509030156},
	keywords = {Activity recognition, Convolutional Neural Networks, Deep learning, Feature extraction, Time series classification},
