	keywords = {Bayesian spatio-temporal analysis, latent Gaussian variables, Non-homogeneous hidden Markov model, rainfall, statistical downscaling},
	pages = {543--568},
	file = {PDF:/Users/a6159737/Zotero/storage/UJUIBXGT/Heaps, Boys, Farrow - Unknown - This work is licensed under a Creative Commons Attribution-NonCommercial 3.0 Unported License Bayesian m.pdf:application/pdf},
}

@article{visser_fitting_2002,
	title = {Fitting {Hidden} {Markov} {Models} to {Psychological} {Data}},
