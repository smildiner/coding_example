	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/bimj.201700223},
	keywords = {clustered data, health status assessment, nonignorable dropout, random-effects approach},
	pages = {962--978},
	file = {Full Text PDF:/Users/a6159737/Zotero/storage/LHZSZIDQ/Montanari et al. - 2018 - A multilevel latent Markov model for the evaluatio.pdf:application/pdf;Snapshot:/Users/a6159737/Zotero/storage/GU493IRQ/bimj.html:text/html},
}

